{
    "name": "geosocio/entity-utils",
    "description": "Utilities for Doctrine Entities",
    "type": "library",
    "license": "LGPL-3",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
          "GeoSocio\\EntityUtils\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "GeoSocio\\EntityUtils\\Tests\\": "tests/"
      }
    },
    "require-dev": {
        "doctrine/collections": "^1.0",
        "doctrine/orm": "^2.0",
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^6.2"
    },
    "scripts": {
        "lint": [
          "phpcs --colors ."
        ],
        "test": [
            "@lint",
            "phpunit -v"
        ],
        "test-coverage": [
            "@lint",
            "phpunit -v --coverage-clover coverage.clover"
        ]
    }
}
